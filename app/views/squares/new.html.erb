<h1>Ny ruta</h1>

<% form_for(@square) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :card_id %><br />
    <%= f.text_field :card_id %>
  </p>
  <p>
    <%= f.label :claim_id %><br />
    <%= f.text_field :claim_id %>
  </p>
  <p>
    <%= f.submit 'Skapa' %>
  </p>
<% end %>

<%= link_to 'Tillbaka', squares_path %>